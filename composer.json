{
    "name":        "netcommons/install",
    "description": "Install Plugin for NetCommons3",
    "type":        "cakephp-plugin",
    "keywords":    ["cakephp", "install"],
    "homepage":    "http://www.netcommons.org/",
    "license":     "NetCommons License",
    "installors": [
        {
            "name":     "NetCommons Community",
            "homepage": "https://github.com/NetCommons3/Install/graphs/contributors"
        }
    ],
    "extra": {
        "installer-paths": {
            "app/Plugin/{$name}": ["type:cakephp-plugin"]
        }
    },
    "require": {
        "cakedc/migrations":            "~2.2",
        "netcommons/m17n":              "dev-master",
        "netcommons/net-commons":       "dev-master",
        "netcommons/users":             "dev-master"
    },
    "config": {
        "vendor-dir": "vendors",
        "process-timeout": 3000
    }
}
